{
    "name": "yiqiesuifeng/laradmin",
    "type": "project",
    "description": "Laravel admin",
    "keywords": [
        "laravel",
        "admin",
        "cms"
    ],
    "authors": [
        {
            "name": "一切随风",
            "email": "chenxinfang.john@gmail.com",
            "homepage": "https://yiqiesuifeng.cn/"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "alibabacloud/sdk": "^1.8",
        "caouecs/laravel-lang": "~3.0",
        "dingo/api": "^2.4.0",
        "fideloper/proxy": "^4.0",
        "geoip2/geoip2": "~2.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "mews/captcha": "^3.0",
        "spatie/laravel-pjax": "^2.0",
        "tymon/jwt-auth": "1.0.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "secure-http": false
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
